// In the table cell for mitigations:
<Box sx={{ pl: 1 }}>
  {["A", "B", "C", "D", "E"].map((key) => {
    const val = entry.mitigations[key];
    if (!val) return null;

    // Check for bullet like "1a." or "2b." at start
    const bulletMatch = val.match(/^(\d+[a-eA-E]?\.)\s*/);
    return (
      <Typography variant="body2" sx={{ mb: 0.5 }} key={key}>
        {bulletMatch ? (
          <>
            <b>{bulletMatch[1]}</b> {val.slice(bulletMatch[0].length)}
          </>
        ) : val}
      </Typography>
    );
  })}
</Box>
