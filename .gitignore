  useEffect(() => {
    axios
      .get(config.url.API_URL + "GetResponseHeader")
      .then((r) => {
        const { first, last } = r.data;
        setName({
          first: first || "NULL",
          last: last || "NULL"
        });
      })
      .catch(() => setName({ first: "NULL", last: "NULL" }));
  }, []);
