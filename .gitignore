<Box sx={{ pl: 1 }}>
  {(() => {
    const val = entry.riskInsight;
    if (!val) return null;
    const match = val.match(/^(\d+\.)\s*/);
    return (
      <Typography variant="body2" sx={{ mb: 0.5 }}>
        {match ? (
          <>
            <b>{match[1]}</b>{val.slice(match[0].length)}
          </>
        ) : val}
      </Typography>
    );
  })()}
</Box>

